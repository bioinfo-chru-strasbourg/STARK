(($NO_HEADER)) || header;

####################################################################################################################################
# Checking the input parameter
#--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
if [ -z "$FASTQ" ] && \
	[ -z "$RUNS" ] && \
	[ -z "$ANALYSIS" ] && \
