					[ $i == "Description" ] && SAMPLE_DESCRIPTION_FIELD=$C
					[ $i == "Sample_Project" ] && SAMPLE_PROJECT_FIELD=$C
					[ $i == "Manifest" ] && MANIFEST_INDEX_FIELD=$C
					C=$(( $C + 1 )) ;
