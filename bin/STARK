					ENV_OLD=$(find_app "$MAIN_APP_OLD" "$STARK_FOLDER_APPS")
					[ "$ENV_OLD" != "" ] && MAIN_APP=$MAIN_APP_OLD
				fi;

				[ "$MAIN_APP" == "" ] && MAIN_APP="default"

				(($VERBOSE)) && echo "#[INFO] Main APP: $MAIN_APP"

