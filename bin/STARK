if (($PIPELINES_INFOS)); then

	echo ""
	echo "###################"
	echo "#### PIPELINES ####"
	echo "###################"
	echo ""

	PIPELINES_INFOS=$TMP_FOLDER_TMP/$RANDOM$RANDOM.pipelines
	MAKEFILE_PARAM=$TMP_FOLDER_TMP/$RANDOM$RANDOM.param
