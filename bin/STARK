					#echo "APP_ONE=$APP_ONE"
					#RUN_CONFIG_FILE=$R/$APP_ONE.analysis.json
					RUN_CONFIG_FILE=$TMP_FOLDER_TMP_R/$APP_ONE.analysis.json
					#RUN_CONFIG_FILE_LIST="$RUN_CONFIG_FILE_LIST$RUN_CONFIG_FILE"
					> $RUN_CONFIG_FILE
					echo "{" >> $RUN_CONFIG_FILE
