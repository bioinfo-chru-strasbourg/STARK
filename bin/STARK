					echo "}" >> $RUN_CONFIG_FILE

					(($DEBUG)) && echo "#[INFO] " && cat $RUN_CONFIG_FILE
					(($DEBUG)) && echo "#[INFO] " && $PYTHON2 $STARK_FOLDER_BIN/json_to_options.py $RUN_CONFIG_FILE

