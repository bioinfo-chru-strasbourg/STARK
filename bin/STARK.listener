	done
	DOCKER_CMD_PARAMETERS=$DOCKER_CMD_PARAMETERS" $DOCKER_MOUNT "

	(($VERBOSE)) && echo "#[INFO] Docker mount '$DOCKER_MOUNT'"


	## DOCKER SOCK
	# Link Docker images & containers

	if [ -e /var/run/docker.sock ]; then
