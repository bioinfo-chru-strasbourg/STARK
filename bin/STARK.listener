do
	#echo "$1=$2"
	#echo "Eval opts";
	case "$1" in
		-e|--env|--app|--application)
			APP="$2"
			shift 2
			;;
		--input)
			INPUT_FOLDER="$2"
			INPUT_FOLDER=$(echo $INPUT_FOLDER | tr "," " ")
			shift 2
			;;
		--condition)
			CONDITION="$2"
			CONDITION=$(echo $CONDITION | tr "," " ")
			shift 2
			;;
		--days)
			DAYS="$2"
			shift 2
			;;
		--log)
			LOG="$2"
			shift 2
			;;
		--command)
			COMMAND="$2"
			shift 2
			;;
		-v|--verbose)
			VERBOSE=1
			shift 1
			;;
		-d|--debug)
			VERBOSE=1
			DEBUG=1
			shift 1
			;;
		-n|--release)
			release;
			exit 0
			;;
		-h|--help)
			HELP=1
			usage
			exit 0
			;;
		--no_header)
			NO_HEADER=1
			shift 1
			;;
		--) shift
			break
			;;
		*) 	echo "# Option $1 is not recognized. " "Use -h or --help to display the help." && \
			exit 1
			;;
	esac
done


