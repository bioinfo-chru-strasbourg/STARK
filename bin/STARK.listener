			(($DEBUG)) && echo "#[INFO] DOCKER CMD: $DOCKER_CMD"
			eval $DOCKER_CMD
		fi;

		exit 0
	fi;

	### COMMAND
	# Create and execute command

	DOCKER_CMD="docker run $DOCKER_CMD_PARAMETERS $DOCKER_STARK_IMAGE_INPUT $PARAM_CMD"
	(($DEBUG)) && echo "#[INFO] DOCKER CMD: $DOCKER_CMD"
	echo ""
